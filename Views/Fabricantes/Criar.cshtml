@model FabricantesModel

@{
    ViewData["Title"] = "Adicionar fabricante";
}


<div class="text-center">
    <h1 class="display-4">Adicionar fabricante</h1>
</div>
<form asp-controller="Fabricantes" asp-action="Criar" method="post">

    @Html.Partial("Forms.cshtml")

    <button type="submit" class="btn btn-primary" style="margin-top:10px">Adicionar</button>
    <a class="btn btn-secondary" asp-action="Index" asp-controller="Fabricantes" role="button" style="margin-top:10px">Voltar</a>

</form>





