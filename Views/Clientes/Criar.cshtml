@model Intelectah.ViewModel.ClientesViewModel

@{
    ViewData["Title"] = "Adicionar Cliente";
}

<div class="text-center">
    <h1 class="display-4">Adicionar Cliente</h1>
</div>
<form asp-controller="Clientes" asp-action="Criar" method="post">
    @Html.Partial("Forms.cshtml")

    <button type="submit" class="btn btn-primary" style="margin-top:10px">Adicionar</button>
    <a class="btn btn-secondary" asp-action="Index" asp-controller="Clientes" role="button" style="margin-top:10px">Voltar</a>
</form>
